<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eddyvalencia.myfitapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityDiario" />
        <activity android:name=".ActivityObjetivos" />
        <activity android:name=".ActivityEditMacronProteinas" />
        <activity android:name=".ActivityCreateAlimento" />
        <activity android:name=".ActivityCreateReceta" />
        <activity android:name=".ActivityCreateEjercicios" />
        <activity
            android:name=".ActivityListAlimentos"
            android:label="@string/title_activity_list_alimentos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivityListRecetas"
            android:label="@string/title_activity_list_recetas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivityListEjercicios"
            android:label="@string/title_activity_list_ejercicios"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>